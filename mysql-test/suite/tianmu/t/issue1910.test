DROP DATABASE IF EXISTS issue1910;
CREATE DATABASE issue1910;
USE issue1910;
--enable_warnings

CREATE TABLE t1 (`id` BIGINT(20) NOT NULL, 
                 `created_date` DATETIME DEFAULT NULL,
                 `name` VARCHAR(20),
                 `price` DECIMAL(10,2));
