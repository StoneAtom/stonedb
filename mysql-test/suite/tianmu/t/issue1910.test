(1,'aaa',186.43),
(2,'bbb',12.2),
(3,'aaa',1743.19),
(4,'ddd',12.2),
(3,'ccc',1743.19);

SELECT C.* FROM t1 A INNER JOIN (SELECT @row1 := @row1 + 1 AS '上划日' FROM (SELECT 0) a) C ON 1 = 1;
SELECT * FROM (SELECT 1) a INNER JOIN t1;
SELECT * FROM (t1 INNER JOIN (SELECT 1) a ON t1.id = a.1) INNER JOIN t2 ON t1.id = t2.id;
SELECT * FROM (SELECT t1.name FROM t1 INNER JOIN t2 ON t1.id = t2.id) a INNER JOIN (SELECT 'aaa',186.43) b ON a.name = 'aaa';
SELECT * FROM (SELECT t1.name FROM t1 INNER JOIN t2 ON t1.id = t2.id) a INNER JOIN (SELECT * FROM (SELECT * FROM t2
WHERE name = 'aaa') b WHERE id = 3) c ON a.name = 'aaa';

