(3,'2023-01-25 17:10:20','ccc',1743.19),
(4,'2012-03-18 23:56:41','ddd',1.3);

CREATE TABLE t2 (`id` INT(20) NOT NULL, 
                 `name` VARCHAR(20),
                 `price` DECIMAL(10,2));
